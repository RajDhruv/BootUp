<%case from%>
<%when :edit%>
$("#playground").html("<%=j render partial:'edit_form'%>");
$("#edit_profile_message").click();
<%when :profile_image_carousel%>
$('#upload_area').fadeOut();
$("#upload_area").html("<%=j render partial:'profile_image_carousel'%>");
$("#upload_area").fadeIn();
$('#upload_area').css('background-color', 'rgba(128, 128, 128, 0.5)');
<%when :make_dp%>
$('#profile_picture').attr("src","<%=@image.location.to_s%>");
<%when :image%>
$('#profile_picture').attr("src","<%=@image.location.to_s%>");
<%end%>